;
try {
/* module-key = 'com.lucidchart.confluence.plugins.lucid-confluence:lucidchart-macro-editor-resources', location = 'js/macro-edit-mode.js' */
(function() {
    var lucidHelper = {
        getBaseUrl: function() {
            return window.location.protocol + '//' + window.location.host + AJS.contextPath();
        },
        showIFrameLoading: function() {
            jQuery('#lucidFrame').attr('src',lucidHelper.getBaseUrl() + '/plugins/lucidchart/loading.action');
            jQuery('#lucidFrame').show();
        },
        showIFrame: function(url) {
            if(jQuery('#lucidFrame').length != 0) {
                jQuery('#lucidFrame').attr('src', url);
                jQuery('#lucidFrame').show();
            }
            else {
                jQuery('<iframe id="lucidFrame" name="lucidFrame">').appendTo('body');
                jQuery('#lucidFrame').attr('src', url);
                jQuery('#lucidFrame').attr('style', 'position:fixed;top:0px;left:0px;background-color:white; z-index: 9999;');
                jQuery('#lucidFrame').attr('height', '100%');
                jQuery('#lucidFrame').attr('width', '100%');
                setTimeout(function() {
                    jQuery('#lucidFrame').focus();
                }, 100);
            }
        },
        hideIFrame: function() {
            jQuery('#lucidFrame').attr('src', "");
            jQuery('#lucidFrame').hide();
        },
        findInQueryString: function(matcher) {
            return $.inArray(matcher, location.search.slice(1).split('&')) >= 0;
        }
    }
    AJS.bind('init.rte', function initialize(event) {
        function doInit($) {

            var lucidPopup;
            var confluencePageId = AJS.params.contentId;

            var diagramId;
            var diagramName;
            var macroDomNode;
            var oldMacroParams;

            /**
             * Opens an iframe to the editor for a given Lucidchart document.
             * @param  {[type]} lucidchartData Macro data provided by Confluence that identifies
             *                                 this diagram
             */
            var lucidchartEdit = function(lucidchartData) {
                if (!(lucidchartData && lucidchartData['params'])) {
                    macroDomNode = null;
                }
                oldMacroParams = lucidchartData && lucidchartData['params'];
                var iframeUrl = lucidHelper.getBaseUrl() + '/plugins/lucidchart/editModeEnsureAccess.action?contentId=' + encodeURIComponent(AJS.params.contentId);
                if (lucidchartData && lucidchartData['params'] && lucidchartData['params']['id']) {
                    iframeUrl += '&documentId=' + lucidchartData['params']['id'];
                }
                lucidHelper.showIFrame(iframeUrl);
            };

            /**
             * Parses a raw string of key-value pairs in the format 'key1=value1|key2=value2|...'
             * and returns an object with those key-value mappings. If any key appears in
             * the string multiple times, only the last value will be used.
             *
             * Expected params as of Lucidchart plugin v1.19:
             *  - align
             *  - height
             *  - id
             *  - name
             *  - pages-to-display
             *  - rich-viewer
             *  - width
             *
             * @param  {string}          params A string in the format 'key1=value1|key2=value2|...'
             * @return {Object.<string>}        A dictionary of the resulting key-value pairs
             */
            var parseMacroParams = function(params) {
                var result = {};
                var keyValueStrings = params.split('|');

                $.each(keyValueStrings, function(_, keyValuePair) {
                    var parts = keyValuePair.split('=');

                    var key = parts.shift();
                    var value = parts.join('=');
                    result[key] = value;
                });

                return result;
            };

            var showDialog = function(macroParams, macroNode) {
                if (!lucidPopup) {
                    buildDialog();
                }

                repopulateDialog(macroParams);
                storeSaveVariables(macroParams, macroNode);
                lucidPopup.show();
            };

            var showError = function(title, message) {
                var error = AJS.messages.error({
                   title: title,
                   body: '<p>' + message + '</p>'
                });
                error.css({'margin-left': '20px', 'margin-right': '20px'});
                error.insertAfter('#content-title-div');
                $('#wysiwygTextarea_ifr').contents().scrollTop(0);
            }

            var buildDialog = function() {
                lucidPopup = new AJS.Dialog(300, 400);
                lucidPopup.addHeader('Diagram Properties');
                lucidPopup.addPanel('Properties', generatePropertyPanelHtml());
                lucidPopup.addSubmit('Save', saveMacroChanges); // like .addButton() but with special css styling
                lucidPopup.addCancel('Cancel', function(dialog) {
                    dialog.hide();
                });
            };

            var generatePropertyPanelHtml = function() {
                // TODO: Add mouse-over text to explain what each of these fields are for. Particularly,
                // give examples of how to format the Pages field.
                var editPropsStr = '<form>';
                editPropsStr += '<p><label>Width: </label><input type="text" name="width" id="props_width" value="0" /></p>';
                editPropsStr += '<p><label>Height: </label><input type="text" name="height" id="props_height" value="0" /></p>';
                editPropsStr += '<p><label>Autosize: </label><br /><input type="radio" name="autofitSelect" id="LucidAutofitOn" value="false">On</input><input type="radio" name="autofitSelect" id="LucidAutofitOff" value="true">Off</input></p>';
                editPropsStr += '<p><label>Align: </label><br /><input type="radio" name="align" id="LucidLeft" value="Left">Left</input><input type="radio" name="align" id="LucidCenter" value="Center">Center</input><input type="radio" name="align" id="LucidRight" value="Right">Right</input></p>';
                editPropsStr += '<p><label>Pages: </label><input type="text" name="pages" id="LucidPages" value="" /></p>';
                editPropsStr += '<p><label>Use Rich Lucidchart viewer: </label><br /><input type="radio" name="richViewSelect" id="LucidRichViewer" value="true">On</input><input type="radio" name="richViewSelect" id="LucidSimpleViewer" value="false">Off</input></p>';
                editPropsStr += '</form>';

                return editPropsStr;
            };

            var saveMacroChanges = function(dialog) {
                var dialogParams = getDialogParams();
                var macroParams = parseMacroParams($(macroDomNode).attr('data-macro-parameters'));
                dialogParams['border'] = macroParams['border'];
                sendSaveAjax(dialogParams, true, macroParams['name']);
                dialog.hide();
            };

            var sendSaveAjax = function(params, update, name) {

                /**
                 * We don't want other diagrams to be able to step on the toes of this save attempt.
                 * Therefore, we need to have our own in-memory copy of what the dialog parameters
                 * were at the time of saving.
                 */
                var dialogParams = params || getDialogParams();
                var macroNode = macroDomNode;

                jQuery.ajax({
                    async: true,
                    url: lucidHelper.getBaseUrl() + "/plugins/lucidchart/diagramAttacher.action",
                    data: {
                        'documentId': diagramId,
                        'accessDenied': false,
                        'error': false,
                        'contentId': confluencePageId,
                        'pages-to-display': dialogParams['pages-to-display'],
                        'update': update,
                        'name': name
                    },
                    timeout: 60000, // 60 seconds TODO: Make configurable
                    success: function(data) {
                        if ((dialogParams['width'] == null && dialogParams['height'] == null) || dialogParams['autofit'] == 'true') {
                            dialogParams['width'] = data['width'];
                            dialogParams['height'] = data['height'];
                        }
                        attachNewMacro(data, dialogParams, macroNode);
                    },
                    error: function(data) {
                        AJS.log('Failed to update Lucidchart diagram ' + diagramName, data);
                        // Release our hold on the editor's save, preview, and cancel buttons
                        Confluence.Editor.UI.setButtonsState(true);
                        deactivateLoadingGif(macroNode);

                        showError('Error updating diagram', data['error']);
                    }
                });

                // We don't want the user to leave the editor before the diagram is updated or
                // it won't have its settings changed. This disables the 'save', 'preview', and
                // 'cancel' buttons.
                Confluence.Editor.UI.setButtonsState(false);
                activateLoadingGif(macroNode);
            };

            var attachNewMacro = function(data, newMacroParams, macroNode) {
                AJS.log('Lucidchart attach request successful. Data:', data);

                newMacroParams['id'] = data['documentId'];
                newMacroParams['name'] = data['baseName'];

                var macroRenderRequestBody = {
                    'contentId': confluencePageId,
                    'macro': {
                        'name': 'lucidchart',
                        'params': newMacroParams,
                        'defaultParameterValue': '',
                        'body': ''
                    }
                };

                deactivateLoadingGif(macroNode);

                // Save the user's current curser location
                AJS.Rte.BookmarkManager.storeBookmark();
                // The editor's save, preview, and cancel buttons are released automatically in
                // .insertMacro().
                tinymce.confluence.MacroUtils.insertMacro(macroRenderRequestBody, macroNode);
                AJS.Rte.BookmarkManager.restoreBookmark();
            };

            var activateLoadingGif = function(macroNode) {
                $(macroNode).addClass('lucid-loading');
            };

            var deactivateLoadingGif = function(macroNode) {
                $(macroNode).removeClass('lucid-loading');
            };

            var getDialogParams = function() {
                var i;
                params = {
                    'width': $('#props_width')[0].value,
                    'height': $('#props_height')[0].value,
                    'pages-to-display': $('#LucidPages')[0].value,
                    'align': '',
                    'rich-viewer': $('#LucidRichViewer')[0].checked.toString(),
                    'autofit': $('#LucidAutofitOn')[0].checked.toString(),
                };

                var alignButtons = document.getElementsByName('align');
                for (i = 0; i < alignButtons.length; i++) {
                    if (alignButtons[i].checked) {
                        params['align'] = alignButtons[i].value;
                        break;
                    }
                }

                return params;
            };

            var repopulateDialog = function(macroParams) {
                $('#props_width').val(macroParams['width']);
                $('#props_height').val(macroParams['height']);
                $('#LucidPages').val(macroParams['pages-to-display']);

                $.each(document.getElementsByName('align'), function(_, button) {
                    button.checked = false;
                });
                if (macroParams['align']) {
                    $('#Lucid' + macroParams['align'])[0].checked = true;
                }

                $.each(document.getElementsByName('richViewSelect'), function(_, button) {
                    button.checked = false;
                });
                if (macroParams['rich-viewer'] == 'true') {
                    $('#LucidRichViewer')[0].checked = true;
                }
                else if (macroParams['rich-viewer'] == 'false') {
                    $('#LucidSimpleViewer')[0].checked = true;
                }

                $.each(document.getElementsByName('autofitSelect'), function(_, button) {
                    button.checked = false;
                });
                if (macroParams['autofit'] == 'true') {
                    $('#LucidAutofitOn')[0].checked = true;
                }
                else {
                    $('#LucidAutofitOff')[0].checked = true;
                }
            };

            var storeSaveVariables = function(macroParams, macroNode) {
                diagramId = macroParams['id'];
                diagramName = macroParams['name'];
                macroDomNode = macroNode;
            };

            /**
             * Overrides the default behavior when the 'Edit' button is clicked in the macro
             * panel.
             */
            AJS.MacroBrowser.setMacroJsOverride('lucidchart', { opener: lucidchartEdit });

            /**
             * Provides behavior for when the user clicks on the 'Properties' button in the macro
             * panel. This button is created because of settings in atlassian-plugin.xml.
             */
            AJS.Confluence.PropertyPanel.Macro.registerButtonHandler('properties', function(_, macroNode) {
                var params = parseMacroParams($(macroNode).attr('data-macro-parameters'));
                showDialog(params, macroNode);
            });

            AJS.Confluence.PropertyPanel.Macro.registerButtonHandler('resize-small', function(_, macroNode) {
                updateMacroSize(300, 300, macroNode, false);
            });

            AJS.Confluence.PropertyPanel.Macro.registerButtonHandler('resize-medium', function(_, macroNode) {
                updateMacroSize(600, 600, macroNode, false);
            });

            AJS.Confluence.PropertyPanel.Macro.registerButtonHandler('resize-large', function(_, macroNode) {
                updateMacroSize(900, 900, macroNode, false);
            });

            AJS.Confluence.PropertyPanel.Macro.registerButtonHandler('resize-original', function(_, macroNode) {
                updateMacroSize(null, null, macroNode, true);
            });

            AJS.Confluence.PropertyPanel.Macro.registerButtonHandler('lucid-server-border', function(_, macroNode) {
                var params = parseMacroParams($(macroNode).attr('data-macro-parameters'));
                var newBorderVal;
                if (params['border'] === 'false') {
                    newBorderVal = 'true';
                } else {
                    newBorderVal = 'false';
                }
                updateMacroSize(params['width'], params['height'], macroNode, params['autofit'], newBorderVal);
            });

            var updateMacroSize = function(width, height, macroNode, newAutofitVal, newBorderVal) {
                var params = parseMacroParams($(macroNode).attr('data-macro-parameters'));
                params['width'] = width;
                params['height'] = height;
                params['autofit'] = newAutofitVal;
                params['border'] = newBorderVal;
                storeSaveVariables(params, macroNode);
                sendSaveAjax(params, true, params['name']);
            }

            window.addEventListener('message', function(message) {
                function parseMessage(queryStringMessage) {
                    var response = {};
                    var vars = queryStringMessage.split('&');
                    for (var i = 0; i < vars.length; i++) {
                        var pair = vars[i].split('=');
                        var key = decodeURIComponent(pair[0]);
                        var value = decodeURIComponent(pair[1]);
                        response[key] = value;
                    }
                    return response;
                }

                var messageObj = parseMessage(message.data);

                if (messageObj['hide'] && messageObj['hide'] == 'true') {
                    lucidHelper.hideIFrame();
                }

                if (messageObj['documentId']) {
                    lucidHelper.showIFrameLoading();
                    var docId = messageObj['documentId'];

                    if(docId == 'confluence') { // the message was for OnPrem add-on, not for us
                        return;
                    }

                    var ajaxRequestSettings = {
                        async: true,
                        url: lucidHelper.getBaseUrl() + '/plugins/lucidchart/diagramAttacher.action',
                        data: { documentId: docId, accessDenied: false, error: false, contentId: AJS.params.contentId, 'pages-to-display': oldMacroParams ? oldMacroParams['pages-to-display'] : '' },
                        timeout: 5 * 60 * 1000,
                        success: function(data) {
                            if (data['baseName'] && data['documentId']) {
                                var macroRenderRequest = {
                                    contentId : Confluence.Editor.getContentId(),
                                    macro : {
                                        name : 'lucidchart',
                                        params :  {
                                            'width': data["width"],
                                            'height': data["height"],
                                            'align': "Left",
                                            'name': data["baseName"],
                                            'id': data["documentId"],
                                            'rich-viewer': data["rich-viewer"],
                                            'autofit':'true'
                                        },
                                        defaultParameterValue : '',
                                        body : ''
                                    }
                                };

                                if (oldMacroParams) {
                                    if (!oldMacroParams['autofit'] || oldMacroParams['autofit'] == 'false') {
                                        macroRenderRequest['macro']['params']['width'] = oldMacroParams['width'];
                                        macroRenderRequest['macro']['params']['height'] = oldMacroParams['height'];
                                        macroRenderRequest['macro']['params']['autofit'] = 'false';
                                    }
                                    if (oldMacroParams['pages-to-display']) {
                                        macroRenderRequest['macro']['params']['pages-to-display'] = oldMacroParams['pages-to-display'];
                                    }
                                }

                                oldMacroParams = null;

                                if(macroDomNode) {
                                    AJS.Rte.getEditor().selection.select(macroDomNode);
                                }

                                AJS.Rte.BookmarkManager.storeBookmark();
                                tinymce.confluence.MacroUtils.insertMacro(macroRenderRequest);
                                AJS.Rte.BookmarkManager.restoreBookmark();
                            } else {
                                AJS.error('Error: could not find required diagram macro response parameters', data);
                            }
                            lucidHelper.hideIFrame();
                        },
                        error: function(xhr, textStatus,errorThrown) {
                            lucidHelper.hideIFrame();
                            AJS.error('Lucidchart attach diagram request failed', xhr, textStatus, errorThrown);
                            var data = JSON.parse(xhr.responseText);
                            showError('Error inserting diagram', data['error']);
                        }
                    };
                    jQuery.ajax(ajaxRequestSettings);
                }
            }, false);

            if (lucidHelper.findInQueryString('showLucidchartDocsList=true')) {
                lucidchartEdit();
            }

            AJS.bind('created.property-panel', function (event, panel) {
                var placeholderEl = $(panel['anchor']);
                var propertyPanel = $(panel['panel']);

                if(placeholderEl.length) {
                    macroDomNode = placeholderEl.get(0);
                }

                if (placeholderEl.data('macro-name') == 'lucidchart') {
                    //Make the small/med/large buttons into icons.
                    propertyPanel.find('.macro-property-panel-resize-small').addClass('image-size-small');
                    propertyPanel.find('.macro-property-panel-resize-medium').addClass('image-size-medium');
                    propertyPanel.find('.macro-property-panel-resize-large').addClass('image-size-large');

                    //depress the border button if a border will be displayed
                    var params = parseMacroParams($(macroDomNode).attr('data-macro-parameters'));
                    if (!(params['border'] === 'false')) {
                        propertyPanel.find('.macro-property-panel-lucid-server-border').addClass('selected');
                    }
                }
            });

            // AJS.log("AJS.version=" + AJS.version);

        };


        if (AJS.MacroBrowser) {
            doInit(AJS.$);
        } else {
            var checkInitEditPageMode = setInterval(function() {
                if (AJS.MacroBrowser) {
                    doInit(AJS.$);
                    clearInterval(checkInitEditPageMode);
                }
            }, 250);
        }
    });

})();

}catch(e){WRMCB(e)};
;
try {
/* module-key = 'com.lucidchart.confluence.plugins.lucid-confluence:lucidchart-global-resources', location = 'js/blueprint.js' */
AJS.toInit(function() {

    Confluence.Blueprint.register(
        'com.lucidchart.confluence.plugins.lucid-confluence:create-page-with-diagram',
        function(createDialog, spaceKey) {
            window.location = Confluence.getContextPath() + '/pages/createpage.action?' +
                'spaceKey=' + encodeURIComponent(spaceKey) +
                (createDialog.getParentPageId() ? '&fromPageId=' + createDialog.getParentPageId() : '') +
                '&showLucidchartDocsList=true';
        }
    )

});

}catch(e){WRMCB(e)};
;
try {
/* module-key = 'com.lucidchart.confluence.plugins.lucid-confluence:lucidchart-global-resources', location = 'js/config.js' */
if (!window['lucidConfigure']){
    window['lucidConfigure'] = {
        'assets_base_url': '../../download/resources/com.lucidchart.confluence.plugins.lucid-confluence:lucid-viewer'
    };
}

}catch(e){WRMCB(e)};
